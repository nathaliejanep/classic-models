<%- include('./partials/head.ejs') %>
<body>
  <%- include('./partials/nav.ejs') %>

    <div class="content">
      <h2>All products</h2>
      <% if (products.length>0){%> <% products.forEach(product =>{ %>
      <div class="info">
        <h3><%= product.productName %></h3>
        <p>$ <%= product.buyPrice %></p>
        <p><%= product.productDescription %></p>
      </div>
      <% }) %> <%}%>
    </div>
  </body>
</html>
